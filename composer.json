{
  "name": "trinitymirror/engage",
  "type": "project",
  "license": "MIT",
  "description": "Engage website",
  "homepage": "http://engage.trinitymirror.com/",
  "authors": [
    {
      "name": "Michael Bragg",
      "email": "michael@michaelbragg.net",
      "homepage": "http://www.michaelbragg.net"
    }
  ],
  "keywords": [
    "wordpress",
    "composer",
    "wp"
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "http://wpackagist.org"
    }
  ],
  "require": {
    "php": ">=5.4",
    "johnpbloch/wordpress": "4.2.x",
    "vlucas/phpdotenv": "~2.0",
    "composer/installers": "~1.0",
    "wpackagist-plugin/wordpress-seo": "2.3.x",
    "wpackagist-plugin/google-analytics-for-wordpress": "5.4.x",
    "wpackagist-plugin/google-sitemap-generator": "4.0.x",
    "wpackagist-plugin/wp-robots-txt": "1.1",
    "wpackagist-plugin/wp-security-audit-log": "2.0.x",
    "wpackagist-plugin/brute-force-login-protection": "1.5.x",
    "wpackagist-plugin/sf-move-login": "2.1.x",
    "wpackagist-plugin/simple-301-redirects": "1.06",
    "wpackagist-plugin/w3-total-cache": "0.9.x"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "~2.3",
    "wp-coding-standards/wpcs": "2014-12-11",
    "wpackagist-plugin/debug-bar": "0.8.x",
    "wpackagist-plugin/developer": "1.2.x",
    "wpackagist-plugin/log-deprecated-notices": "0.3.x",
    "wpackagist-plugin/p3-profiler": "1.5.x",
    "wpackagist-plugin/theme-check": "20141222.1",
    "wpackagist-plugin/user-switching": "1.0.x"
  },
  "extra": {
    "wordpress-install-dir": "web/system",
    "installer-paths": {
      "web/content/plugins/{$name}/": ["type:wordpress-plugin"],
      "web/content/mu-plugins/{$name}/": ["type:wordpress-muplugin"],
      "web/content/themes/{$name}/": ["type:wordpress-theme"]
    }
  }
}
